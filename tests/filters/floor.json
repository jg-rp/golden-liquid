{
  "tests": [
    {
      "name": "positive integer",
      "template": "{{ 5 | floor }}",
      "result": "5"
    },
    {
      "name": "negative integer",
      "template": "{{ -5 | floor }}",
      "result": "-5"
    },
    {
      "name": "positive float",
      "template": "{{ 5.4 | floor }}",
      "result": "5"
    },
    {
      "name": "negative float",
      "template": "{{ -5.4 | floor }}",
      "result": "-6"
    },
    {
      "name": "zero",
      "template": "{{ 0 | floor }}",
      "result": "0"
    },
    {
      "name": "positive string float",
      "template": "{{ \"5.1\" | floor }}",
      "result": "5"
    },
    {
      "name": "negative string float",
      "template": "{{ \"-5.1\" | floor }}",
      "result": "-6"
    },
    {
      "name": "unexpected argument",
      "template": "{{ -3.1 | floor: 1 }}",
      "invalid": true
    },
    {
      "name": "string not a number",
      "template": "{{ \"hello\" | floor }}",
      "result": "0"
    },
    {
      "name": "not a string, int or float",
      "template": "{{ a | floor }}",
      "data": {
        "a": {}
      },
      "result": "0"
    },
    {
      "name": "undefined left value",
      "template": "{{ nosuchthing | floor }}",
      "result": "0"
    }
  ]
}