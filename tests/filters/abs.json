{
  "tests": [
    {
      "name": "positive integer",
      "template": "{{ 5 | abs }}",
      "result": "5"
    },
    {
      "name": "negative integer",
      "template": "{{ -5 | abs }}",
      "result": "5"
    },
    {
      "name": "positive float",
      "template": "{{ 5.4 | abs }}",
      "result": "5.4"
    },
    {
      "name": "negative float",
      "template": "{{ -5.4 | abs }}",
      "result": "5.4"
    },
    {
      "name": "zero",
      "template": "{{ 0 | abs }}",
      "result": "0"
    },
    {
      "name": "positive string integer",
      "template": "{{ '5' | abs }}",
      "result": "5"
    },
    {
      "name": "negative string integer",
      "template": "{{ '-5' | abs }}",
      "result": "5"
    },
    {
      "name": "positive string float",
      "template": "{{ '5.1' | abs }}",
      "result": "5.1"
    },
    {
      "name": "negative string float",
      "template": "{{ '-5.1' | abs }}",
      "result": "5.1"
    },
    {
      "name": "unexpected argument",
      "template": "{{ -3 | abs: 1 }}",
      "invalid": true
    },
    {
      "name": "string not a number",
      "template": "{{ 'hello' | abs }}",
      "result": "0"
    },
    {
      "name": "not a string, int or float",
      "template": "{{ a | abs }}",
      "data": {
        "a": {}
      },
      "result": "0"
    },
    {
      "name": "undefined left value",
      "template": "{{ nosuchthing | abs }}",
      "result": "0"
    }
  ]
}