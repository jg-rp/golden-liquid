{
  "tests": [
    {
      "name": "range literal last filter left value",
      "template": "{{ (1..3) | last }}",
      "result": "3"
    },
    {
      "name": "array of strings",
      "template": "{{ arr | last }}",
      "data": {
        "arr": [
          "a",
          "b"
        ]
      },
      "result": "b"
    },
    {
      "name": "array of things",
      "template": "{{ arr | last }}",
      "data": {
        "arr": [
          "a",
          "b",
          1,
          [],
          {}
        ]
      },
      "result": "{}"
    },
    {
      "name": "empty array",
      "template": "{{ arr | last }}",
      "data": {
        "arr": []
      },
      "result": ""
    },
    {
      "name": "left value not an array",
      "template": "{{ arr | last }}",
      "data": {
        "arr": 12
      },
      "result": ""
    },
    {
      "name": "left value is undefined",
      "template": "{{ nosuchthing | last }}",
      "result": ""
    },
    {
      "name": "last of a string",
      "template": "{{ 'hello' | last }}",
      "result": ""
    },
    {
      "name": "last of a hash",
      "template": "{{ a | last }}",
      "data": {
        "a": {
          "b": 1,
          "c": 2
        }
      },
      "result": ""
    }
  ]
}